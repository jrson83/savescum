{
  "name": "@savescum/web",
  "version": "0.1.2",
  "description": "An interactive webinterface, to efficiently save scum on an exploitable PS4/PS5.",
  "keywords": [
    "bloodborne",
    "ftp",
    "goldhen",
    "playstation4",
    "ps4",
    "save-game",
    "savescum"
  ],
  "homepage": "https://savescum.jrson.me",
  "bugs": {
    "url": "https://github.com/jrson83/savescum/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jrson83/savescum.git",
    "directory": "packages/web"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*": ["lib/index.d.ts"]
    }
  },
  "files": ["dist", "lib"],
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "pnpm clean && tsc && vite build --watch",
    "dev:only": "vite",
    "build": "pnpm clean && tsc && vite build",
    "preview": "pnpm build && vite preview",
    "typecheck": "tsc --noEmit",
    "test": "pnpm typecheck && node --test-reporter=spec --import=tsx/esm ./src/__tests__/basic.test.ts"
  },
  "dependencies": {
    "@preact/signals": "^1.2.2",
    "classix": "^2.1.35",
    "preact": "^10.19.3"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.7.0",
    "@types/node": "^20.10.4",
    "sass": "^1.69.5",
    "typescript": "5.3.3",
    "vite": "^5.0.2"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
